{% extends 'base.html.twig' %}

{% block body %}
    <h1>SurveyResponse list</h1>

    <table>
        <thead>
            <tr>
                <th>Value</th>
                <th>Id</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for surveyResponse in surveyResponses %}
            <tr>
                <td><a href="{{ path('surveyresponse_show', { 'id': surveyResponse.id }) }}">{{ surveyResponse.value }}</a></td>
                <td>{{ surveyResponse.id }}</td>
                <td>
                    <ul>
                        <li>
                            <a href="{{ path('surveyresponse_show', { 'id': surveyResponse.id }) }}">show</a>
                        </li>
                        <li>
                            <a href="{{ path('surveyresponse_edit', { 'id': surveyResponse.id }) }}">edit</a>
                        </li>
                    </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('surveyresponse_new') }}">Create a new entry</a>
        </li>
    </ul>
{% endblock %}
